//	----	bit ops	-----------------------------------------------------------
	
replace restart {
	mov	a, %2
	and		a, #0x01
	ceqsn	a, #0x00
	goto	%1
} by {; peephole ( bit-wait %2 0 )
	t0sn	%2, #0
	goto	%1
} //if notUsed('a')

