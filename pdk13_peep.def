//	----	bit movements	-----------------------------------------------
 
replace restart {
	t0sn.io	f, %4
	goto	%2
	mov.io	%1, %5
	or		%1, #0x01
	goto	%3
%2:
	mov.io	%1, %5
	and		%1, #0xfe
%3:
	mov.io	%5, %1
} by {	; peephole ( t0sn bit set/clear )
	t0sn.io	f, %4
	goto	%2
	set1.io	%5, #0
	goto	%3
%2:
	set0.io	%5, #0
%3:
}

