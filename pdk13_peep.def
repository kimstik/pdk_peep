//	----	misc	-----------------------------------------------------------
replace restart {
	goto	%1
%1:
} by {	; peephole ( jmp +1 )
%1:
}

replace restart {
	call	%1
	mov		%2, a
	call	%3
	mov		%4, a
	mov		a, %2
	sub		a, %5
	mov		a, %4
	subc	a, %6
} by {	; peephole ( const sub16 )
	call	%1
	sub		a, %5
	call	%3
	subc	a, %6
}//if notUsed(%2)

