//	PDK13 peephole rules

//	----	conditional jumps	-----------------------------------------------
 
replace restart {
%1:
	t0sn	%3,%4
	goto	%2
	goto	%1
} by {	; peephole ( untwist conditional jumps back t0sn -> t1sn )
%1:
	t1sn	%3,%4
	goto	%1
	goto	%2
}

replace restart {
	t0sn	%3,%4
	goto	%2
	goto	%1
%2:
} by {	; peephole ( untwist conditional jumps forward t0sn -> t1sn )
	t1sn	%3,%4
	goto	%1
	goto	%2
%2:
}

replace restart {
%1:
	t0sn.io	%3,%4
	goto	%2
	goto	%1
} by {	; peephole ( untwist conditional jumps back t0sn -> t1sn )
%1:
	t1sn.io	%3,%4
	goto	%1
	goto	%2
}

replace restart {
	t0sn.io	%3,%4
	goto	%2
	goto	%1
%2:
} by {	; peephole ( untwist conditional jumps forward t0sn -> t1sn )
	t1sn.io	%3,%4
	goto	%1
	goto	%2
%2:
}
  
